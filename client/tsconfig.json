{
  "compilerOptions": {
    "types": [
      "babylonjs",
      "babylonjs-loaders"
    ],
    "typeRoots": [
      "node_modules/@types",
      "./@types"
    ],
    "module": "ES2020",
    "esModuleInterop": true,
    "target": "ES2020",
    "moduleResolution": "node",
    "sourceMap": false,
    "jsx": "react"
  },
  "lib": [
    "es2015"
  ],
  "exclude": [
    "./node_modules"
  ]
}